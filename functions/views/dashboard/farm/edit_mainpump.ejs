<div id="content">
<div id="content-header">
  <div id="breadcrumb"> <a href="index.html" title="Go to Home" class="tip-bottom"><i class="icon-home"></i> Home</a> <a href="#" class="tip-bottom">จัดการฮาร์ดแวร์</a> <a href="#" class="current">รายการปั๊มน้ำ</a> </div>
  <h1>ปั๊มน้ำหลัก</h1>
</div>
<div class="container-fluid">
  <hr>
  <div class="row-fluid">
    <div class="span6">
      <div class="widget-box">
        <div class="widget-title"> <span class="icon"><i class="icon-th"></i></span>
          <h5>ปั๊มน้ำหลัก (Main Pump)</h5>
        </div>
        <div class="widget-content nopadding">
          <table class="table table-bordered table-striped table-hover data-table">
            <thead class="thead-inverse">
              <tr>
                <th>หมายเลขปั๊ม</th>
                <th>Description</th>
                <th>Write API Key</th>
                <th>field1 Status</th>
                <th>field2 Status</th>
                <th>แก้ไขข้อมูล</th>
              </tr>
            </thead>
            <tbody>
              <% for(var i=0;i<mainpumps.length;i++) {%>
              <tr class="gradeX">
                <td><%= mainpumps[i].id %></td>
                <td>...</td>
                <td><%= mainpumps[i].write_api_key %></td>
                <td class="center">-</td>
                <td class="center">-</td>
                <td align="center"><button class="btn btn-warning btn-mini" onclick="">แก้ไข</button></td>
              </tr>
              <% } %>
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
  <form action="addMainPump" method="post">
  <div class="form-row align-items-center">
  <div class="col-auto">
  <label class="sr-only" for="inlineFormInput">หมายเลขปั๊มน้ำ</label>
  <input type="text" class="form-control mb-2" id="inlineFormInput" name="pump_id" placeholder="12345">
  </div>
  <div class="col-auto">
  <label class="sr-only" for="inlineFormInputGroup">Write API Key</label>
  <div class="input-group mb-2">
  <div class="input-group-prepend">
  </div>
  <input type="text" class="form-control" id="inlineFormInputGroup" name="write_api_key" placeholder="B0D0LDXXXYHBO9VF">
  </div>
  </div>
  <div class="col-auto">
  <button type="submit" class="btn btn-primary mb-2">เพิ่ม</button>
  </div>
  </div>
  </form>
</div>
